int main(void)
{
	int n;
	int last_digit;
 
	srand(time(0));
	n = rand() - RAND_MAX / 2;
	/* your code goes there */
	last_digit = n % 10;
	if(last_digit > 5)
	  {
	    printf("Last digit of d% is greater than 5
	}
	return (0);
}